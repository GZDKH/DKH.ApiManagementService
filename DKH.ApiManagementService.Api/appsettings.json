{
  "Application": "DKH.ApiManagementService",
  "Platform": {
    "Logging": {
      "MinimumLevel": "Information",
      "OverrideMinimumLevel": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=dkh_api_management;Username=postgres;Password=postgres;Include Error Detail=true;"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
